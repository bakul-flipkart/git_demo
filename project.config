[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Bakul Agarwal (bakul-flipkart)
[access "refs/for/refs/*"]
	read = group user/Bakul Agarwal (bakul-flipkart)
	push = group user/Bakul Agarwal (bakul-flipkart)
	label-Code-Review = -2..+2 group user/Bakul Agarwal (bakul-flipkart)
	label-Verified = -1..+1 group user/Bakul Agarwal (bakul-flipkart)
	rebase = group user/Bakul Agarwal (bakul-flipkart)
	removeReviewer = group user/Bakul Agarwal (bakul-flipkart)
	submit = group user/Bakul Agarwal (bakul-flipkart)
[access "refs/heads/*"]
	read = group user/Bakul Agarwal (bakul-flipkart)
	create = group user/Bakul Agarwal (bakul-flipkart)
	push = +force group user/Bakul Agarwal (bakul-flipkart)
	pushMerge = group user/Bakul Agarwal (bakul-flipkart)
[access "refs/tags/*"]
	push = +force group user/Bakul Agarwal (bakul-flipkart)
	pushTag = group user/Bakul Agarwal (bakul-flipkart)
	pushSignedTag = group user/Bakul Agarwal (bakul-flipkart)
